Algoritmo "Notas"
Var
   nota1, nota2, NF : real
Inicio
   escreva("Digite a primeira nota: ")
   leia(nota1)
   escreva("Digite a segunda nota: ")
   leia(nota2)
   NF <- (nota1 + nota2)
   escreval("NOTA FINAL = ", NF)
   se (NF < 60.00) entao
      escreval("REPROVADO")
   senao
   fimse
Fimalgoritmo
