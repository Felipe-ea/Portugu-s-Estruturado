Algoritmo "crescente"

Var

   x, y: inteiro

Inicio


   
   x <- 0
   y <- 1

   enquanto x <> y faca
      escreval("Escreva dois numeros: ")
      leia(x)
      leia(y)
      
      se x < y entao
         escreval("CRESCENTE")
       senao
          escreval("DECRESCENTE")
        fimse
   fimenquanto


Fimalgoritmo

